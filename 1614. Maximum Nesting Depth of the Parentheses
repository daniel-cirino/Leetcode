class Solution:
    def maxDepth(self, s: str) -> int:
        stack = []
        res = 0
        
        for i in range(len(s)):
            if s[i] == '(':
                stack.append('(')
            if s[i] == ')' and len(stack) != 0:
                if len(stack) > res:
                    res = len(stack)
                stack.pop()
        return res
                
        
