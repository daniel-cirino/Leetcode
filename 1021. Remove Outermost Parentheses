
    
class Solution:
    def removeOuterParentheses(self, s: str) -> str:
        res = []
        stack = []
        level = 0
        for i in s:
            if i == '(' and len(stack) != 0:
                res.append(i)

            if i == "(":
                stack.append('(')
            elif i == ")":
                stack.pop()


            if i == ")" and len(stack) != 0:
                res.append(i)

        return "".join(res)
