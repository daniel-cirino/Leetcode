class Solution:
    def numJewelsInStones(self, jewels: str, stones: str) -> int:
        count = 0
        for i in range(len(stones)):
            if stones[i] in jewels:
                count += 1
        return count
        
class Solution:
    def numJewelsInStones(self, J: str, S: str) -> int:
        res, count = 0, {}
        
        for l in S:
            if l in count: count[l] += 1
            else: count[l] = 1
        
        for l in J:
            if l in count:
                res += count[l]
        
        return res
