# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def middleNode(self, head):
        slow = fast = head
        while fast and fast.next:
            slow = slow.next
            fast = fast.next.next
        return slow
            
        # fast = head [0]
        # fast = fast.next.next goes two places
        # fast = head[2]
        # fast = head[4]
        # fast = head[6]
        # slow = head[1]
        # slow = head[2]
        # slow = head[3]
